{
  "name": "jira-github-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js --source-map --license licenses.txt",
    "transpile": "babel-changed --src dist --dest transpiled --source-maps false",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "engines": {
    "node": "12.16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-gohri/jira-github-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "integration",
    "jira"
  ],
  "author": "Rohit Gohri <work@rohit.page>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^5.0.0",
    "isomorphic-fetch": "^3.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@smpx/babel-changed": "^0.5.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
